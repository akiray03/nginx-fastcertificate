# TODO: change to resolver address
resolver 169.254.169.254 valid=5s;

upstream named_reverse_proxy {
  server unix:/var/run/named_reverse_proxy.sock;
}

server {
  listen unix:/var/run/named_reverse_proxy.sock;
  server_name _;

  set $origin "test2.example.com";

  location / {
    proxy_pass https://$origin:443;
  }
}
